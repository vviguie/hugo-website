@article{roman_2003_OscillatoryFracturePaths,
 abstract = {We report a novel mode of quasi-static oscillatory crack propagation when a cutting tip of moderately large width is driven through a thin brittle polymer film. Experiments show that the amplitude and wavelength of the oscillatory crack paths scale linearly with the width of the cutting tip over a wide range of length scales but are independent of the width of the sheet and of the cutting speed. We propose a mechanism for this instability, based on the coupling between crack propagation and out-of-plane deformations of the film. To cite this article: B.~Roman et~al., C. R. Mecanique 331 (2003). Résum\ ́Nous pr\'éntons une nouvelle instabilit\'eścillante de fissure qui se produit lorsqu'un indenteur assez large est forc\'e  ́travers une feuille mince fragile pr\'ealĺement entaill\'ee. ś exp\'erienś r\'ev\l̀e ́que l'amplitude et la longueur d'onde de la fissure d\'ependenĺin\'eairemend́e largeur de l'indenteur sur une grande gamme de largeurs, et ne d\'ependent  ́de la largeur du film, ni de la vitesse de d\'ecoupe. No ́proposons un m\'ecanisme po ́cette instabilit\'e, fond\'e s ́un coupǵe entre l'avanc\'ee de la fissu ́et les d\'eformations trav́erses du film. Pour citer cet article : B.~Roman et~al., C. R. Mecanique 331 (2003).},
 author = {Roman, B. and Reis, P. M. and Audoly, B. and De Villiers, S. and Viguié, V. and Vallet, D.},
 doi = {10.1016/j.crme.2003.10.002},
 issn = {1631-0721},
 journal = {Comptes Rendus Mécanique},
 keywords = {Fissure,Fissure oscillante,Fracture,Oscillatory crack path,Plaques élastiques minces,Rupture,Thin elastic plates},
 langid = {english},
 month = {December},
 number = {12},
